sudo ufw status
sudo ufw status numbered
sudo ufw status verbose
sudo ufw show added


#Delete
sudo ufw status numbered
sudo ufw delete 2
sudo ufw status numbered

sudo ufw allow 80/tcp
sudo ufw allow 443/tcp
sudo ufw deny 23/tcp

sudo ufw delete allow 80/tcp
sudo ufw delete allow 443/tcp
sudo ufw delete deny 23/tcp


sudo ufw disable
sudo ufw enable
sudo ufw reset
